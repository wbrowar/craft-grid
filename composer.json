{
    "name": "wbrowar/craft-grid",
    "description": "A field that lets you content manage CSS Grid in Craft CMS.",
    "type": "craft-plugin",
    "version": "1.2.3",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "grid"
    ],
    "support": {
        "docs": "https://github.com/wbrowar/craft-grid/blob/master/README.md",
        "issues": "https://github.com/wbrowar/craft-grid/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Will Browar",
            "homepage": "http://wbrowar.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.2.5"
    },
    "autoload": {
        "psr-4": {
          "wbrowar\\grid\\": "src/"
        }
    },
    "extra": {
        "name": "Grid",
        "handle": "grid",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/wbrowar/craft-grid/master/CHANGELOG.md",
        "components": {
            "grid": "wbrowar\\grid\\services\\GridService"
        },
        "class": "wbrowar\\grid\\Grid"
    }
}
